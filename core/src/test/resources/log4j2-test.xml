<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="STDOUT">
            <PatternLayout pattern="[%p] [%t] %m%n"/>
        </Console>
        <List name="api-coverage-listAppender"/>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="STDOUT"/>
        </Root>
        <Logger name="api-coverage-logger" level="info">
            <AppenderRef ref="api-coverage-listAppender"/>
        </Logger>
    </Loggers>
</Configuration>
